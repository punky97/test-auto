{
  "name": "Autopilot",
  "description": "Autopilot",
  "version": "1.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["storage", "cookies", "webRequest"],
  "host_permissions": ["https://autopilot.shopbase.dev/*"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "/icons/jenkins_48.png",
      "32": "/icons/jenkins_48.png",
      "48": "/icons/jenkins_48.png",
      "128": "/icons/jenkins_48.png"
    }
  }
}
